                            <Property name="model" type="javax.swing.table.TableModel" editor="org.netbeans.modules.form.RADConnectionPropertyEditor" postCode="JComboBox&lt;String&gt; p_cbb_tx_promo = new JComboBox&lt;&gt;();&#xa;fillCbbTxPromo(p_cbb_tx_promo);&#xa;TableColumn colTxPromo = jt_promo.getColumn(&quot;Tx promo&quot;);&#xa;colTxPromo.setCellEditor(new DefaultCellEditor(p_cbb_tx_promo));&#xa;DefaultTableCellRenderer centerPromoRenderer = new DefaultTableCellRenderer();&#xa;centerPromoRenderer.setHorizontalAlignment( JLabel.CENTER );&#xa;jt_promo.setDefaultRenderer(Object.class, centerPromoRenderer);&#xa;jt_promo.setDefaultRenderer(Integer.class, centerPromoRenderer);&#xa;//jt_promo.setDefaultRenderer(Boolean.class, centerPromoRenderer);&#xa;jt_promo.setDefaultRenderer(Double.class, centerPromoRenderer);&#xa;//Listener d&apos;&#xe9;coute du tableau pour le calcul du prix reduit&#xa;jt_promo.getModel().addTableModelListener(new TableModelListener() {&#xa;    public void tableChanged(TableModelEvent e) {&#xa;    //Des que le tableau est modifie, le calcul du prix reduit est mis &#xe0; jour&#xa;        if (e.getType() == TableModelEvent.UPDATE){&#xa;            int columnIndex = e.getColumn();&#xa;            int rowIndex = e.getFirstRow();&#xa;&#xa;            int id_produit = Integer.parseInt(jt_promo.getValueAt(rowIndex, 0).toString());&#xa;            boolean en_promo = (Boolean) jt_promo.getValueAt(rowIndex, 4);&#xa;            if(columnIndex == 5) {&#xa;                double prix = Double.parseDouble(jt_promo.getValueAt(rowIndex, 3).toString());&#xa;                int tx_promo = Integer.parseInt(jt_promo.getValueAt(rowIndex, 5).toString());&#xa;                String tx_promoString = Integer.toString(tx_promo);&#xa;                double tx_promod = Double.parseDouble(tx_promoString);&#xa;                //double tx_promo = Double.ParseDouble(jt_promo.getValueAt(rowIndex, 5);&#xa;                double prix_reduit = prix * (1-(tx_promod/100));&#xa;                jt_promo.setValueAt(prix_reduit, rowIndex, 6);&#xa;                Produit p_promo = new Produit (id_produit, en_promo, tx_promo, prix_reduit);&#xa;                updateProduitPromo(p_promo);&#xa;            }&#xa;            /*if(columnIndex == 4) {&#xa;                if(!en_promo) {&#xa;                jt_promo.setValueAt(false, rowIndex, 4);&#xa;                jt_promo.setValueAt(0, rowIndex, 5);&#xa;                jt_promo.setValueAt(0, rowIndex, 6);&#xa;                Produit p = new Produit (id_produit, false, 0, 0);&#xa;                updateProduitPromo(p);&#xa;                }&#xa;            }*/&#xa;        }&#xa;    }&#xa;});">
